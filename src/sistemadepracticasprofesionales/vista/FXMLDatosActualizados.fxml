<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="460" prefWidth="600" style="-fx-background-color: #F5F7FA;" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sistemadepracticasprofesionales.controlador.FXMLDatosActualizadosController">
  
  <children>
    <AnchorPane prefHeight="60.0" prefWidth="600.0" style="-fx-background-color: #487BB1;">
      <children>
        <Label layoutX="20.0" layoutY="15.0" text="Confirmar Actualización de Proyecto" textFill="WHITE">
          <font>
            <Font name="Segoe UI Bold" size="24.0" />
          </font>
        </Label>
      </children>
    </AnchorPane>

    <VBox layoutX="30.0" layoutY="80.0" spacing="10.0">
      <children>
        <Label fx:id="lblNombre" text="Nombre: ">
               <font>
                  <Font size="16.0" />
               </font></Label>
        <TextArea fx:id="taDescripcion" editable="false" prefHeight="60.0" promptText="Descripción" wrapText="true">
               <font>
                  <Font size="13.0" />
               </font></TextArea>
        <Label fx:id="lblEstado" text="Estado: ">
               <font>
                  <Font size="16.0" />
               </font></Label>
        <Label fx:id="lblCupo" text="Cupo: ">
               <font>
                  <Font size="16.0" />
               </font></Label>
        <Label fx:id="lblFechaInicio" text="Fecha de Inicio: ">
               <font>
                  <Font size="16.0" />
               </font></Label>
        <Label fx:id="lblFechaFin" text="Fecha de Fin: ">
               <font>
                  <Font size="16.0" />
               </font></Label>
        <Label fx:id="lblOrganizacion" text="Organización Vinculada: ">
               <font>
                  <Font size="16.0" />
               </font></Label>
        <Label fx:id="lblResponsable" text="Responsable del Proyecto: ">
               <font>
                  <Font size="16.0" />
               </font></Label>
      </children>
    </VBox>

    <HBox layoutX="400.0" layoutY="400.0" spacing="10.0">
      <children>
        <Button fx:id="btnAceptar" onAction="#btnClicAceptar" style="-fx-background-color: #487BB1;" text="Aceptar" textFill="WHITE">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font></Button>
        <Button fx:id="btnCancelar" onAction="#btnClicCancelar" style="-fx-background-color: #487BB1;" text="Cancelar" textFill="WHITE">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font></Button>
      </children>
    </HBox>

  </children>
</AnchorPane>
